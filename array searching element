#include <stdio.h>

int main()
{
    int size,a;
    scanf("%d",&size);
    int arr[size];
    for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
    int key;
    scanf("%d",&key);
    for(int index=0;index<size;index++)
    
    {
        if(arr[index]==key){
        printf("%d",index);
        a=1;
        }
    }
    if(a!=1)
    {
    printf("%d",-1);
    }


    return 0;
}
